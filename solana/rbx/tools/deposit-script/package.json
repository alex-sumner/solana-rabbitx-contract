{
  "name": "rbx",
  "version": "0.1.0",
  "license": "ISC",
  "private": true,
  "type": "module",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "test": "anchor test",
    "build": "anchor build",
    "deploy": "anchor deploy",
    "test:deposits": "ANCHOR_PROVIDER_URL=http://localhost:8899 ANCHOR_WALLET=~/.config/solana/id.json ts-node --esm tests/simple_deposit_test.js",
    "init:program": "ANCHOR_PROVIDER_URL=http://localhost:8899 ANCHOR_WALLET=~/.config/solana/id.json ts-node --esm tests/initialize_script.js",
    "start:validator": "solana-test-validator --reset",
    "setup:all": "anchor build && anchor deploy && npm run init:program && npm run test:deposits",
    "show:logs": "solana logs -u localhost",
    "deploy:devnet": "ts-node --esm scripts/deploy-devnet.ts",
    "init:devnet": "ts-node --esm scripts/init-devnet.ts",
    "test:withdrawal:devnet": "ts-node --esm scripts/test-withdrawal-devnet.ts",
    "deposit:devnet": "ts-node --esm scripts/make-deposit.ts",
    "direct:init": "scripts/direct-init.sh",
    "direct:deposit": "scripts/direct-deposit.sh",
    "custom:init": "ts-node --esm scripts/custom-init.ts",
    "custom:deposit": "ts-node --esm scripts/custom-deposit.ts",
    "fix:init": "ts-node --esm scripts/fix-init.ts",
    "fix:deposit": "ts-node --esm scripts/fix-deposit.ts",
    "simple:init": "ts-node --esm scripts/simple-init.ts",
    "test:init": "ts-node --esm scripts/test-init.ts",
    "test:deposit": "ts-node --esm scripts/test-deposit.ts",
    "anchor:init": "ts-node --esm scripts/anchor-init.ts",
    "anchor:deposit": "ts-node --esm scripts/anchor-deposit.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/spl-token": "^0.4.12",
    "@solana/web3.js": "^1.98.0",
    "bn.js": "5.2.1",
    "elliptic": "^6.6.1",
    "js-sha3": "^0.9.3"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.5",
    "chai": "4.3.7",
    "ethers": "^6.13.5",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
